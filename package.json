{
  "name": "sanity-plugin-icon-picker",
  "version": "2.0.2",
  "description": "An Icon Picker plugin for Sanity",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "sanity.json",
    "src"
  ],
  "scripts": {
    "build": "plugin-kit verify-package && pkg-utils build",
    "watch": "pkg-utils watch --strict",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopherafbjur/sanity-plugin-icon-picker.git"
  },
  "keywords": [
    "sanity",
    "plugin",
    "icon",
    "custom input",
    "sanity field"
  ],
  "author": "Christopher Af Bjur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christopherafbjur/sanity-plugin-icon-picker/issues"
  },
  "homepage": "https://github.com/christopherafbjur/sanity-plugin-icon-picker#readme",
  "dependencies": {
    "@sanity/icons": "^2.2.2",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/plugin-kit": "^3.1.7",
    "@sanity/ui": "^1.3.0",
    "decamelize": "6.0.0",
    "framework7-icons": "^5.0.5",
    "react-icons": "^4.7.1",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@sanity/pkg-utils": "^2.2.6",
    "npm-run-all": "^4.1.5"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "scripts": false,
      "babelConfig": false,
      "sanityV2Json": false,
      "tsconfig": false
    }
  },
  "source": "./src/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.js",
      "source": "./src/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.mjs",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.esm.mjs",
  "types": "./dist/index.js",
  "engines": {
    "node": ">=14"
  },
  "type": "commonjs"
}
